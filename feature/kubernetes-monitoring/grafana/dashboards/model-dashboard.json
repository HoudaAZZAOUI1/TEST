{
  "dashboard": {
    "title": "Model Performance Dashboard",
    "panels": [
      {
        "title": "Recommendation Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(recommendation_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 Latency"
          }
        ]
      },
      {
        "title": "Recommendations per Second",
        "targets": [
          {
            "expr": "rate(recommendations_total[5m])",
            "legendFormat": "Recommendations/sec"
          }
        ]
      },
      {
        "title": "Model Accuracy",
        "targets": [
          {
            "expr": "model_accuracy",
            "legendFormat": "Accuracy"
          }
        ]
      }
    ]
  }
}

